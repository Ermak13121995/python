mounth = int(input('Введите номер месяца: '))
days = 0

def day_count(num):
    if 1 <= num <= 12:
        if num == 4 or num == 6 or num == 9 or num == 11 and num != 2:
            days = 30
        else:
            days = 31
        
        if num == 2:
            days = 28
            
    return days

result = day_count(mounth)
    
print(result)
