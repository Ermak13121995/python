maximum = 0
minimum = 0

number = int(input('Введите число >= 2: '))

if number >= 2:
   for i in range(number):
      a = int(input())
      
      if maximum == 0 or a > maximum:
         maximum = a

      if minimum == 0 or a < minimum:
         minimum = a

   print('Минимум равен: ', minimum)
   print('Максимум равен: ', maximum)
      
else:
   print('Введённое число меньше двух')
