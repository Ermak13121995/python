print('Введите первое число')
a = int(input())
print('Введите второе число')
b = int(input())
c = a * b
d = a / b
e = a // b
f = a % b
j = a**b

print(a, 'умноженное на', b, 'равно', c)
print(a, 'делённое на', b, 'равно', d)
print(a, 'нацело делённое на ', b, 'равно', e)
print('Остаток от деления', a, 'на ', b, 'равно', f)
print(a, 'в степени', b, 'равно', j)
